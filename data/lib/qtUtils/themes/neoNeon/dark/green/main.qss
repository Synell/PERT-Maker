/* QDropDownWidget */

QPushButton.QDropDownWidget {
	background-color: transparent;
	color: #fefefe;
	border: none;
	margin: 0 0 0 0;
	spacing: 0;
	qproperty-icon: url(./data/lib/qtUtils/themes/neoNeon/dark/green/icons/dropdownwidget/rightArrow.png) off,
        			url(./data/lib/qtUtils/themes/neoNeon/dark/green/icons/dropdownwidget/downArrow.png) on;
	qproperty-iconSize: 30px;
}

QPushButton.QDropDownWidget:pressed {
	color: #fefefe;
}







