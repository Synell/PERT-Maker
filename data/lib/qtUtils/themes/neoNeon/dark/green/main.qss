}



/* QToolBar */

QToolBar {
	background-color: #31363b;
	border: 1px solid #4f5b62;
}



/* QToolButton */   /*TODO:*/

QToolButton {
	color: #fefefe;
	background-color: transparent;
	border: 1px solid transparent;
	border-radius: 8px;
	margin: 0px 1px;
}

QToolButton:hover {
	background-color: #24009ceb;
}

QToolButton:pressed {
	background-color: #32009ceb;
	padding-left: 1px;
	padding-top: 1px;
}



/* QTabWidget */   /*TODO:*/

QTabWidget::tab-bar {
	background-color: transparent;
}

QTabWidget::pane { 
	position: absolute;
	border: 1px solid #fefefe;
	background-color: transparent;
}

QTabBar::tab {
	background-color: #232629;
	color: #fefefe;
	border: 1px solid transparent;
	padding: 3 10 3 10;
}

QTabBar::tab:hover {
	background-color: #5e5e5e;
	border: 1px solid #fefefe;
}

QTabBar::tab:selected {
	background-color: #777777;
	border: 1px solid transparent;
}



/* QSideBar */   /*TODO:*/

QWidget.QSideBar {
	background-color: #232629;
	border: none;
	margin: 0 0 0 0;
	spacing: 0;
}

QPushButton.QSideBar {
	background-color: #232629;
	color: #fefefe;
	border: none;
	border-radius: 0px;
	height: 50px;
	text-align: left;
	font-size: 24px;
	qproperty-iconSize: 60px;
	padding-left: -1px;
}

QPushButton.QSideBarSeparator {
	background-color: #232629;
	color: #fefefe;
	border: none;
	border-radius: 0px;
	height: 20px;
	text-align: center;
	font-size: 24px;
	padding-left: -1px;
}

QPushButton:hover.QSideBar {
	background-color: #555555;
	color: #fefefe;
}

QPushButton:pressed.QSideBar {
	background-color: #666666;
	color: #fefefe;
}

QPushButton:focus.QSideBar {
	border: none;
}



/* QSidePanel */   /*TODO:*/

QWidget.QSidePanel {
